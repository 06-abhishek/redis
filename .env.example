# Redis Configuration
REDIS_HOST=127.0.0.1   # Redis server hostname or IP address (localhost for local development)
REDIS_PORT=6379        # Redis server port (default is 6379)

# Redis Authentication (Optional)
REDIS_PASSWORD=        # Redis password (leave empty if no password is set)

# Rate Limiting
RATE_LIMIT_WINDOW=15   # Rate limit time window in minutes
RATE_LIMIT_MAX=100     # Max requests allowed in the window per IP

# BullMQ (Job Queue) Configuration
BULL_QUEUE_NAME=myQueue  # Name of the Redis queue for background tasks
